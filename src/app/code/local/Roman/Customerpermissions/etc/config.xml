<config>
    <modules>
        <Roman_Customerpermissions>
            <version>1.0.0</version>
        </Roman_Customerpermissions>
    </modules>
    <global>
        <resources>
            <customerpermissions_setup>
                <setup>
                    <module>Roman_Customerpermissions</module>
                </setup>
            </customerpermissions_setup>
        </resources>
        <helpers>
            <roman_customerpermissions>
                <class>Roman_Customerpermissions_Helper</class>
            </roman_customerpermissions>
        </helpers>
    </global>
    <admin>
        <routers>
            <customerpermissions>
                <use>standard</use>
                <args>
                    <module>Roman_Customerpermissions</module>
                    <frontName>customerpermissions</frontName>
                </args>
            </customerpermissions>
        </routers>
    </admin>
